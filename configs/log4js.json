{
    "log": {
        "_comment": "logシステムの設定です。よくわからない場合無視してください",
        "appenders": {
            "system": { 
                "type": "dateFile",
                "filename": "logs/system.log" 
            }, 
            "console": {
                "type": "console"
            }
        },
        "categories": {
            "default": { 
                "appenders": ["system", "console"], 
                "level": "all" 
            } 
        }
    }
}